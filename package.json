{
  "name": "react-starter",
  "version": "1.0.0",
  "repository": "https://gitlab.zengenti.com/starter-projects/react-starter",
  "license": "None",
  "description": "",
  "main": "index.js",
  "config": {
    "startup": ""
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js",
    "build": "rimraf ./dist && cross-env NODE_ENV=production parallel-webpack --config webpack/webpack.config.prod.js && npm run build:startup",
    "build:startup": "buildstartup",
    "server": "node --max-http-header-size=800000 dist/start.js",
    "dev:server": "cross-env NODE_ENV=test NODE_OPTIONS=--max-http-header-size=80000 babel-node -x .ts,.tsx,.js,.jsx --inspect=9229 src/server/debug.js",
    "debug:server": "nodemon --delay 500ms --watch \"src/**\" --ext \"ts,tsx,js,jsx,json\" --signal SIGTERM --exec \"kill-port 3001 && npm run dev:server\"",
    "build:server": "npm run build && npm run server",
    "build:dev:server": "npm run build && npm run dev:server",
    "analyze": "rimraf ./dist && cross-env NODE_ENV=production ANALYZE=TRUE webpack --config webpack/webpack.config.prod.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest  -c ./config/jest.config.js --detectOpenHandles --forceExit",
    "test:watch": "jest --config=./config/jest.config.js --watch",
    "test:coverage": "jest --config=./config/jest.config.js --coverage",
    "prebuild:storybook": "npm run test:generate-output",
    "storybook": "start-storybook -p 6006",
    "storybook-ci": "start-storybook --ci -p 6006",
    "storybook-static": "build-storybook -c .storybook -o .out",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": "http://Zengenti.com",
  "devDependencies": {
